public class Book {
	String name;
	long ISBN;
	String author;
	String publisher;
	String BookInfo;
	Book(String name,long isbn,String a,String p){
		this.name=name;
		this.ISBN=isbn;
		this.author=a;
		this.publisher=p;}
		void setname(String name) {
			this.name=name;
		}
		void setISBN(long isbn) {
			this.ISBN=isbn;
		}
		void setauthor(String a) {
			this.author=a;
		}
		void setpublisher(String p) {
			this.publisher=p;
		}
		String getname() {
			return name;
		}
		long getISBN() {
			return ISBN;
		}
		String getauthor() {
			return author;
		}
		String getpublisher() {
			return publisher;
		}
		String getBookInfo() {
			System.out.println("book name is:"+name);
			System.out.println("ISBN number is:"+ISBN);
			System.out.println("author name is:"+author);
			System.out.println("publisher is:"+publisher);
		}
	}

public class BookTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Book b1[]=new Book[30];
		b1[1]=new Book("wings of fire","1616","APJ Abdul Kalam","Herb");
		b1[1].getBookInfo();

	}

}
